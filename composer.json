{
    "name": "manzwebdesigns/my-journal",
    "description": "A journal application focusing on privacy.",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "erusev/parsedown": "^1.7",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "michaeldegroot/doctrine-encrypt-bundle": "^5.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/deprecation-contracts": "^2.5",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/filesystem": "^4.4|^5.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-ctype": "~1.23",
        "symfony/polyfill-intl-messageformatter": "^1.12",
        "symfony/polyfill-php80": "^1.23",
        "symfony/polyfill-php81": "^1.23",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/rate-limiter": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/security-csrf": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/string": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/webpack-encore-bundle": "^1.4",
        "symfony/yaml": "^6.0",
        "symfonycasts/verify-email-bundle": "^1.6",
        "symfony/html-sanitizer": "^6.0",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "^0.13.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.1.2"
          },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.0",
            "id": "01ESEMDF774TKCCEX894TDZEMN"
        }
    }
}
